<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for prospects -->
        <record id="seq_crm_prospect" model="ir.sequence">
            <field name="name">Prospect Sequence Default</field>
            <field name="code">crm.prospect.seq</field>
            <field name="prefix">PRO-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Mobility -->
        <record id="seq_crm_mobility" model="ir.sequence">
            <field name="name">Mobility Sequence Default</field>
            <field name="code">crm.mobility.seq</field>
            <field name="prefix">MO-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
